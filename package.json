{
  "name": "grimdawn-builds",
  "version": "1.0.0",
  "description": "Grim Dawn Build Compendium",
  "main": "index.js",
  "author": "Morten Lindhardt <r3fuze@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "babel-node index.js --presets es2015,stage-2"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "got": "^6.7.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.11.0",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.4",
    "striptags": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "semicolon": false,
    "space": 4,
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "dot-notation": "off",
      "spaced-comment": "off",
      "object-curly-spacing": "off",
      "no-unused-expressions": "off",
      "no-else-return": "off",
      "no-warning-comments": "off",
      "import/prefer-default-export": "off",
      "import/default": "off",
      "ava/no-todo-test": "off",
      "ava/no-skip-test": "off"
    }
  }
}
